    $('.server-list-menu .getplay a').on('click', function(e) {e.preventDefault();
    $('.server-list-menu .getplay a').removeClass('active');$(this).addClass('active');$('.server-list-menu .dropdown-menu').removeClass('show');$('.server-loader').fadeIn();var code = $(this).data('code');$.ajax({type: 'POST',url: ajaxRequest,data: "action=iframe_server&code="+code,success: function(data) {if(data.status){$('.watch-embed-player').html('<div class="server-loader"></div>');$('.watch-embed-player .server-loader').before(data.codeplay);}$('.server-loader').fadeOut();$('body').trigger('click');},error: function() {}});return false;});
